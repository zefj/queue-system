{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "ter-indent": [
      true,
      4,
      {
        "SwitchCase": 1
      }
    ],
    "max-line-length": [
      true,
      {
        "limit": 120,
        "ignore-pattern": "throw new (.*?);$"
      }
    ],
    "import-name": [
      false
    ],
    "jsx-no-multiline-js": [false],
    "variable-name": [
      true,
      "allow-pascal-case"
    ],
    "jsx-boolean-value": [false]
  },
  "rulesDirectory": []
}
